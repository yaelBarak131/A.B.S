<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<ScrollPane fitToHeight="true" fitToWidth="true" stylesheets="" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="component.LoanBox.LoanBoxController">
   <content>
      <HBox id="hbox" maxHeight="140.0" maxWidth="250.0" minHeight="50.0" minWidth="20.0" prefHeight="140.0" prefWidth="241.0" stylesheets="@LoanBox.css">
         <children>
            <GridPane id="vbox" prefHeight="130.0" prefWidth="231.0">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
                <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="64.0" />
                  <ColumnConstraints hgrow="NEVER" maxWidth="157.0" minWidth="89.0" prefWidth="157.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="10.0" minWidth="0.0" prefWidth="0.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                  <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="ID:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="Owner:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="Category:" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label text="Capital:" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label text="TotalYaz:" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <Label text="Interest:" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <Label text="Status:" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                  <Label fx:id="idLabel" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Label fx:id="ownerLabel" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <Label fx:id="categoryLabel" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                  <Label fx:id="capitalLabel" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                  <Label fx:id="totalYazLabel" GridPane.columnIndex="2" GridPane.rowIndex="5" />
                  <Label fx:id="interestLabel" GridPane.columnIndex="2" GridPane.rowIndex="6" />
                  <Label fx:id="statusLabel" onMouseClicked="#showStatusInfo" GridPane.columnIndex="2" GridPane.rowIndex="7" />
               </children>
            </GridPane>
         </children>
      </HBox>
   </content>
</ScrollPane>
